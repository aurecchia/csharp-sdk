<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net45</TargetFramework>
        <AssemblyName>Wallee</AssemblyName>
        <Title>Wallee</Title>
        <PackageId>Wallee</PackageId>
        <OutputType>Library</OutputType>
        <Authors>customweb GmbH</Authors>
        <Company>customweb GmbH</Company>
        <AssemblyTitle>wallee SDK for C#</AssemblyTitle>
        <Description>wallee SDK for C#</Description>
        <Copyright>Apache-2.0</Copyright>
        <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
        <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <RootNamespace>Wallee</RootNamespace>
        <Version>2.0.1</Version>
        <PackageTags>Wallee;payment;sdk;Payment Integration</PackageTags>
        <PackageIconUrl>https://app-wallee.com/assets/images/wallee-icon-primary.svg</PackageIconUrl>
        <PackageLicense>https://www.apache.org/licenses/LICENSE-2.0.txt</PackageLicense>
        <License>https://www.apache.org/licenses/LICENSE-2.0.txt</License>
        <Icon>https://app-wallee.com/assets/images/wallee-icon-primary.svg</Icon>
        <Summary>wallee SDK for C#</Summary>
        <ReleaseVersion>2.0.1</ReleaseVersion>
        <SynchReleaseVersion>false</SynchReleaseVersion>
        <Owners>customweb</Owners>
        <RepositoryUrl>http://github.com/wallee-payment/csharp-sdk.git</RepositoryUrl>
        <PackageProjectUrl>http://github.com/wallee-payment/csharp-sdk</PackageProjectUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
            <PackageReference Include="RestSharp" Version="105.1.0"/>
        <PackageReference Include="Newtonsoft.Json" Version="11.0.2"/>
        <PackageReference Include="JsonSubTypes" Version="1.3.0"/>
        <PackageReference Include="NuGet.Build.Tasks.Pack" Version="5.2.0" />
    </ItemGroup>

        <ItemGroup>
            <Reference Include="System"/>
            <Reference Include="System.Core"/>
            <Reference Include="System.Xml.Linq"/>
            <Reference Include="System.Data.DataSetExtensions"/>
            <Reference Include="System.ComponentModel.DataAnnotations"/>
            <Reference Include="Microsoft.CSharp"/>
            <Reference Include="System.Data"/>
            <Reference Include="System.Runtime.Serialization"/>
            <Reference Include="System.Xml"/>
        </ItemGroup>

</Project>
